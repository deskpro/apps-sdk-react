{
  "name": "@deskpro/apps-sdk-react",
  "version": "0.1.6",
  "description": "A react-based javascript SDK for building DeskPRO Apps powered by the React library",
  "main": "lib/index.js",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest ./tests/*/*.test.*",
    "build:lib": "rm -rf lib/* && babel --loose es6.modules -d lib/ src/",
    "build:dist": "webpack -p --progress --env.production",
    "build": "npm run build:lib && npm run build:dist",
    "lint": "eslint src/* --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepublish": "npm run build"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/deskpro/apps-sdk-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskpro/apps-sdk-react.git"
  },
  "bugs": {
    "url": "https://github.com/deskpro/apps-sdk-react/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "documentation": "^5.3.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "jsdoc-babel": "^0.3.0",
    "lint-staged": "^4.2.3",
    "module-resolver": "^1.0.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@deskpro/apps-sdk-core": "^1.0.0-beta.24",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-svg-inline": "^2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
